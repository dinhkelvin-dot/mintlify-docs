{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Textiful Voice ",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/images/favicon.ico",
